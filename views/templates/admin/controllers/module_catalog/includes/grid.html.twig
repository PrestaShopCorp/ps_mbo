<div id="modules-list-container-{{ id }}" class="row modules-list" data-name="{{ id }}">
  {% for index, module in modules %}
    {% block addon_card %}
      {% include '@Modules/ps_mbo/views/templates/admin/controllers/module_catalog/includes/card_list.html.twig' with { 'index': index, 'module': module, 'origin': origin|default('none'), 'category': id } %}
    {% endblock %}
  {% endfor %}

  {% block addon_search_card %}
    {% if requireAddonsSearch is defined and requireAddonsSearch == true %}
      {% include '@Modules/ps_mbo/views/templates/admin/controllers/module_catalog/includes/card_grid_addons.html.twig'%}
      {% include '@Modules/ps_mbo/views/templates/admin/controllers/module_catalog/includes/card_list_addons.html.twig'%}
    {% endif %}
  {% endblock %}
</div>
